
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id        String  @id  @default(uuid())
  email     String   @unique
  password  String?
  image     String?

  @@index([email])
}

model Cart{
  id        String  @id  @default(uuid())
  name  String   @unique
  price  String 
  details  String 
  textcolor  String 
  color  String 
  material  String 
  imageUrl  String 
  availabel  String

  @@index([name])
}